{
  "name": "bucket.social",
  "version": "1.0.0",
  "description": "AT Protocol blob management system with Content Addressable Network (CAN) functionality",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "pm2:start": "npm run build && pm2 start ecosystem.config.cjs --only bucket-social-can",
    "pm2:start:prod": "npm run build && pm2 start ecosystem.config.cjs --only bucket-social-can",
    "pm2:start:staging": "npm run build && pm2 start ecosystem.config.cjs --only bucket-social-can-staging",
    "pm2:start:dev": "npm run build && pm2 start ecosystem.config.cjs --only bucket-social-can-dev",
    "pm2:restart": "pm2 restart bucket-social-can",
    "pm2:reload": "pm2 reload bucket-social-can",
    "pm2:stop": "pm2 stop bucket-social-can",
    "pm2:delete": "pm2 delete bucket-social-can",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "logs": "pm2 logs bucket-social-can",
    "logs:clear": "pm2 flush bucket-social-can",
    "logs:dev": "pm2 logs bucket-social-can-dev",
    "logs:staging": "pm2 logs bucket-social-can-staging",
    "deploy:setup": "pm2 deploy ecosystem.config.cjs production setup",
    "deploy:prod": "pm2 deploy ecosystem.config.cjs production",
    "deploy:staging": "pm2 deploy ecosystem.config.cjs staging"
  },
  "keywords": [
    "atprotocol",
    "can",
    "blob",
    "storage",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@atproto/api": "^0.16.2",
    "@types/redis": "^4.0.11",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pm2": "^5.4.3",
    "redis": "^5.8.1"
  }
}
